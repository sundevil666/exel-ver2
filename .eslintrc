{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jest/globals": true
    },
    "rules": {
        "semi": "off",
        "arrow-parens": "off",
        "indent": 2,
        "comma-dangle": "off",
        "require-jsdoc": "off",
        "eol-last": "off",
        "max-len": "off",
        "no-trailing-spaces": "off",
        "space-before-function-paren": "off"
    },
    "plugins": ["jest"],
    "extends": ["eslint:recommended", "google"]
}